def my_function(arr,target,low,high):
    while low<=high:
        mid=(low+high)//2
        if arr[mid]==target:
            return mid
        elif target>arr[mid]:
            low=mid+1
        else:
            high=mid-1
    return -1
arr=[2,6,7,8,9,10]
target=7
low=0
high=len(arr)-1
result=my_function(arr,target,low,high)
if result==-1:
    print("element not found")
else:
    print("found =",result)
